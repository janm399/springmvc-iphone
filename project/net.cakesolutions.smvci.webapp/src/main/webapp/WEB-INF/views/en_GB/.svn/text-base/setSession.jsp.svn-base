<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<link type="text/css" rel="stylesheet" href="/styles/main.css" />
	<link type="text/css" rel="stylesheet" href="/styles/effects.css" />
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.3.2/jquery.min.js"></script>
	<script src="http://cdn.jquerytools.org/1.1.2/tiny/jquery.tools.min.js"></script>
	<script src="/js/tools.overlay.apple-1.0.1.min.js"></script>
	<title>Biodose</title>
</head>
<body>
<div class="container">
	<form:form action="/setSession.html" method="post" commandName="setSessionForm">
		<fieldset title="Set Session Date">
			<legend>Set session date</legend>
			<p>
				<label for="input">Session Date</label><form:input path="sessionDate" cssClass="long"/>
				<input type="submit" value="Set" />
			</p>
		</fieldset>
	</form:form>
</div>
</body>
</html>